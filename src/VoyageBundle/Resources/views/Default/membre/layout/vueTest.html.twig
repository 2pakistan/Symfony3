{% extends ":layout:layout.html.twig" %}


{% block body %}

    <div class="container">
        <section class="see-trip-header header-homepage">
            <h1 class="text-center"> Autocompletion adresse : test</h1>
        </section>
        <section class="see-trip">
                <div class="col-xs-12">
                    <form class="form-horizontal">
                        <input id="autocomplete" placeholder="Enter your address"
                               onFocus="geolocate()" type="text" class="form-control input-lg"/>

                        <label for="country" class="control-label col-sm-2">country</label>
                        <div class="col-xs-10">
                            <input id="country" type="text" class="form-control col-sm-10"/>
                        </div>

                        <label for="locality" class="control-label col-sm-2">place</label>
                        <div class="col-xs-10">
                            <input id="locality" type="text" class="form-control"/>
                        </div>

                        <label for="route" class="control-label col-sm-2">adresse</label>
                        <div class="col-xs-10">
                            <input id="route" type="text" class="form-control"/>
                        </div>

                        <label for="street_number" class="control-label col-sm-2">Num√©ro de voie</label>
                            <div class="col-xs-10">
                                <input id="street_number" type="text" class="form-control"/>
                            </div>
                        <label for="administrative_area_level_1" class="control-label col-sm-2">Region</label>
                        <div class="col-xs-10">
                            <input id="administrative_area_level_1" type="text" class="form-control"/>
                        </div>
                        <label for="latitude" class="control-label col-sm-2">latitude</label>
                        <div class="col-xs-10">
                            <input id="latitude" type="text" class="form-control"/>
                        </div>

                        <label for="longitude" class="control-label col-sm-2">longitude</label>
                        <div class="col-xs-10">
                            <input id="longitude" type="text" class="form-control"/>
                        </div>
                        <label for="postal_code" class="control-label col-sm-2">postal code</label>
                        <div class="col-xs-10">
                            <input id="postal_code" type="text" class="form-control"/>
                        </div>
                    </form>
                </div>
            </div>
        </section>
    </div>


{% endblock %}
{% block javascripts %}
    <script src="{{ asset('js/autocompleteAdress.js') }}"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA8MUOfjQsy5bd9U4L8vYqpGkNjM9qOfYw&signed_in=true&libraries=places&callback=initAutocomplete"
            async defer></script>
{% endblock %}



